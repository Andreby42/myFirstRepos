package com.yinlian.api.app.dto;

public class IpsAppDto {

	// 商户号
		private String merCode;

		private String account;
		
		private String merName;
		// 商户的md5的证书
		private String md5key;

		// 正式环境提交地址
		private String actionUrl;
		//币种 
		private String currency;    
		
		// 支付方式 默认借记卡01 信用卡02 IPS账号支付03
		private String gateway;
		
		// 语言
		private String lang;
		
		// 订单支付接口加密方式 MD5
		private String orderEncode;
		
		// 交易返回接口加密方式 md5
		private String retEncode;
		private String serverUrl;
		
		private String retType;
		private String isCredit;
		private String bankCode;
		private String productType;
		private String version;
		private String billEXP;
		private String msgId;
		
		public String getMerCode() {
			return merCode;
		}
		public void setMerCode(String merCode) {
			this.merCode = merCode;
		}
		public String getAccount() {
			return account;
		}
		public void setAccount(String account) {
			this.account = account;
		}
		public String getMerName() {
			return merName;
		}
		public void setMerName(String merName) {
			this.merName = merName;
		}
		public String getMd5key() {
			return md5key;
		}
		public void setMd5key(String md5key) {
			this.md5key = md5key;
		}
		public String getActionUrl() {
			return actionUrl;
		}
		public void setActionUrl(String actionUrl) {
			this.actionUrl = actionUrl;
		}
		public String getCurrency() {
			return currency;
		}
		public void setCurrency(String currency) {
			this.currency = currency;
		}
		public String getGateway() {
			return gateway;
		}
		public void setGateway(String gateway) {
			this.gateway = gateway;
		}
		public String getLang() {
			return lang;
		}
		public void setLang(String lang) {
			this.lang = lang;
		}
		public String getOrderEncode() {
			return orderEncode;
		}
		public void setOrderEncode(String orderEncode) {
			this.orderEncode = orderEncode;
		}
		public String getRetEncode() {
			return retEncode;
		}
		public void setRetEncode(String retEncode) {
			this.retEncode = retEncode;
		}
		public String getServerUrl() {
			return serverUrl;
		}
		public void setServerUrl(String serverUrl) {
			this.serverUrl = serverUrl;
		}
		public String getRetType() {
			return retType;
		}
		public void setRetType(String retType) {
			this.retType = retType;
		}
		public String getIsCredit() {
			return isCredit;
		}
		public void setIsCredit(String isCredit) {
			this.isCredit = isCredit;
		}
		public String getBankCode() {
			return bankCode;
		}
		public void setBankCode(String bankCode) {
			this.bankCode = bankCode;
		}
		public String getProductType() {
			return productType;
		}
		public void setProductType(String productType) {
			this.productType = productType;
		}
		public String getVersion() {
			return version;
		}
		public void setVersion(String version) {
			this.version = version;
		}
		public String getBillEXP() {
			return billEXP;
		}
		public void setBillEXP(String billEXP) {
			this.billEXP = billEXP;
		}
		public String getMsgId() {
			return msgId;
		}
		public void setMsgId(String msgId) {
			this.msgId = msgId;
		}
		
		
}
