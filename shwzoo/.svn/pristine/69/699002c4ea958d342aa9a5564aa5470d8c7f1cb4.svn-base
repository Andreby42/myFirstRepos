public class CriteriaSendTemplate extends Criteria {

    private Integer type; //模板类型

    private Integer ctype; //内容类型

    /**
     * Getter method for property <tt>type</tt>.
     * 
     * @return property value of type
     */
    public Integer getType() {
        return type;
    }

    /**
     * Setter method for property <tt>type</tt>.
     * 
     * @param type value to be assigned to property type
     */
    public void setType(Integer type) {
        this.type = type;
    }

    /**
     * Getter method for property <tt>ctype</tt>.
     * 
     * @return property value of ctype
     */
    public Integer getCtype() {
        return ctype;
    }

    /**
     * Setter method for property <tt>ctype</tt>.
     * 
     * @param ctype value to be assigned to property ctype
     */
    public void setCtype(Integer ctype) {
        this.ctype = ctype;
    }

}
