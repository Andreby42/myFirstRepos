package com.yinlian.Enums;

/**
 * 会员余额时段
 * 
 * @author mjx
 *
 */
public enum MemberBalanceTimeBucketEnum {
	近三个月收支明细(1), 三个月前收支明细(2);
	private final int value;

	public int getValue() {
		return value;
	}

	// 构造器默认也只能是private, 从而保证构造函数只能在内部使用
	MemberBalanceTimeBucketEnum(int value) {
		this.value = value;
	}
}
