package com.yinlian.api.app.dto;

import java.util.List;

public class Api_CityDto {
	private int cid; // 省市区ID

	private String cname; // 省市区名称

	private String ccode; // 省市区code
	private List<Api_AreaDto> areas;
	public int getCid() {
		return cid;
	}

	public void setCid(int cid) {
		this.cid = cid;
	}

	public String getCname() {
		return cname;
	}

	public void setCname(String cname) {
		this.cname = cname;
	}

	public String getCcode() {
		return ccode;
	}

	public void setCcode(String ccode) {
		this.ccode = ccode;
	}
	public List<Api_AreaDto> getAreas() {
		return areas;
	}

	public void setAreas(List<Api_AreaDto> areas) {
		this.areas = areas;
	}
}
