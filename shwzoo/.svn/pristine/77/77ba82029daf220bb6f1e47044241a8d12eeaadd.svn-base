package com.yinlian.Enums;

/**
 * 会员优惠券状态
 * 
 * @author mjx
 *
 */
public enum MemberCouponTypeEnum {
	全部(0), 未使用(1), 已使用(2), 已过期(3), 回收站(4);
	private int value;

	public int getValue() {
		return value;
	}

	// 构造器默认也只能是private, 从而保证构造函数只能在内部使用
	MemberCouponTypeEnum(int value) {
		this.value = value;
	}
}
