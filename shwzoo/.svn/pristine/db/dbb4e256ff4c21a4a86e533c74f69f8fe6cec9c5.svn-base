package com.yinlian.wssc.web.util;

import java.util.Date;

/**
 * 团购信息查询类
 * @author Administrator
 *
 */
public class CriteriaGroupBuy extends Criteria {

    private Integer id;       //ID主键
    private Integer annouid;  //广告id
    private String  name;     //标题
    private Integer type;     //类型
    private Integer status;   //状态
    private Date    startFrom; //开始时间  开始
    private Date    startTo;  //开始时间  结束
    private Date    endFrom;  //结束时间  开始
    private Date    endTo;    //结束时间  结束
	private Integer userid;      //买家ID
	private Integer position;      //位置
	private String   time;//查询日期
	private Integer detailstatus;   //状态
	 
    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public Integer getType() {
        return type;
    }

    public void setType(Integer type) {
        this.type = type;
    }

    public Integer getStatus() {
        return status;
    }

    public void setStatus(Integer status) {
        this.status = status;
    }

    public Date getStartFrom() {
        return startFrom;
    }

    public void setStartFrom(Date startFrom) {
        this.startFrom = startFrom;
    }

    public Date getStartTo() {
        return startTo;
    }

    public void setStartTo(Date startTo) {
        this.startTo = startTo;
    }

    public Date getEndFrom() {
        return endFrom;
    }

    public void setEndFrom(Date endFrom) {
        this.endFrom = endFrom;
    }

    public Date getEndTo() {
        return endTo;
    }

    public void setEndTo(Date endTo) {
        this.endTo = endTo;
    }

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    /**
     * Getter method for property <tt>annouid</tt>.
     * 
     * @return property value of annouid
     */
    public Integer getAnnouid() {
        return annouid;
    }

    /**
     * Setter method for property <tt>annouid</tt>.
     * 
     * @param annouid value to be assigned to property annouid
     */
    public void setAnnouid(Integer annouid) {
        this.annouid = annouid;
    }

	public Integer getUserid() {
		return userid;
	}

	public void setUserid(Integer userid) {
		this.userid = userid;
	}

	public Integer getPosition() {
		return position;
	}

	public void setPosition(Integer position) {
		this.position = position;
	}

	public String getTime() {
		return time;
	}

	public void setTime(String time) {
		this.time = time;
	}

	public Integer getDetailstatus() {
		return detailstatus;
	}

	public void setDetailstatus(Integer detailstatus) {
		this.detailstatus = detailstatus;
	}

}
