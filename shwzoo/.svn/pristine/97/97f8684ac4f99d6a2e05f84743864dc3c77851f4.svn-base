package com.yinlian.Enums;

/**
 * 数据类型
 * 
 * @author mjx
 *
 */
public enum DateTypeEnum {
	今天(1), 昨天(2), 过去七天(3), 本月(4), 上一个月(5), 其他(6);
	private final int value;

	public int getValue() {
		return value;
	}

	// 构造器默认也只能是private, 从而保证构造函数只能在内部使用
	DateTypeEnum(int value) {
		this.value = value;
	}
}
