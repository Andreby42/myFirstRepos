package com.yinlian.Enums;

/**
 * 市场活动推荐类型
 * 
 * @author mjx
 *
 */
public enum MarketRecommendType {
	主题馆(1), 品牌推荐(2), 精选推荐(3);
	private final int value;

	public int getValue() {
		return value;
	}

	// 构造器默认也只能是private, 从而保证构造函数只能在内部使用
	MarketRecommendType(int value) {
		this.value = value;
	}
}
