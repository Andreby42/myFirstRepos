package com.yinlian.pc.dto;

import java.math.BigDecimal;
import java.util.Date;
import java.util.List;

import com.yinlian.api.app.dto.OrderdetailDto;

public class OrderListDto {

	private Integer Id;
	// 订单编号
	private String code;
	// 订单状态
	private Integer status;
	// 发货时间
	private Date deliverDate;
	// 订单总运费
	private Float freight;
	// 订单总金额
	private BigDecimal price;
	// 实际支付
	private Double actualPay;

	private int iscomment;
	private int isremind;
	// 订单取消或退款原因
	private String reason;

	private String shopname;

	private String shopid;

	private String shoptel;

	private List<OrderdetailDto> children;
	
	public Integer getId() {
		return Id;
	}

	public void setId(Integer id) {
		Id = id;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public Integer getStatus() {
		return status;
	}

	public void setStatus(Integer status) {
		this.status = status;
	}

	public Date getDeliverDate() {
		return deliverDate;
	}

	public void setDeliverDate(Date deliverDate) {
		this.deliverDate = deliverDate;
	}

	public Float getFreight() {
		return freight;
	}

	public void setFreight(Float freight) {
		this.freight = freight;
	}

	public BigDecimal getPrice() {
		return price;
	}

	public void setPrice(BigDecimal price) {
		this.price = price;
	}

	public Double getActualPay() {
		return actualPay;
	}

	public void setActualPay(Double actualPay) {
		this.actualPay = actualPay;
	}

	public int getIscomment() {
		return iscomment;
	}

	public void setIscomment(int iscomment) {
		this.iscomment = iscomment;
	}

	public String getReason() {
		return reason;
	}

	public void setReason(String reason) {
		this.reason = reason;
	}

	public String getShopname() {
		return shopname;
	}

	public void setShopname(String shopname) {
		this.shopname = shopname;
	}

	public String getShopid() {
		return shopid;
	}

	public void setShopid(String shopid) {
		this.shopid = shopid;
	}

	public String getShoptel() {
		return shoptel;
	}

	public void setShoptel(String shoptel) {
		this.shoptel = shoptel;
	}

	public List<OrderdetailDto> getChildren() {
		return children;
	}

	public void setChildren(List<OrderdetailDto> children) {
		this.children = children;
	}

	public int isIsremind() {
		return isremind;
	}

	public void setIsremind(int isremind) {
		this.isremind = isremind;
	}

}
