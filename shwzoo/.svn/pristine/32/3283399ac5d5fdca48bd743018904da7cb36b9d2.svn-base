.goods_list_price{color: #C7000B;padding: 4% 5% 3%;}
/* loading */
#loading{height: 35px;position: relative;padding: 5%;}
#loading-center{position:relative}
#loading-center-absolute{-moz-border-radius:50% 50% 50% 50%;-webkit-border-radius:50% 50% 50% 50%;border-radius:50% 50% 50% 50%;position:absolute;left:50%;top:0px;height:35px;width:35px;margin-left:-28px;}
.object{width:7px;height:7px;background-color:#C7C7C7;position:absolute;-moz-border-radius:50% 50% 50% 50%;-webkit-border-radius:50% 50% 50% 50%;border-radius:50% 50% 50% 50%;-webkit-animation:animate .8s infinite;animation:animate .8s infinite}
#object_one{top:0px;left:17.5px}
#object_two{top:6px;left:31px;-webkit-animation-delay:.1s;animation-delay:.1s}
#object_three{top:17.5px;left:35px;-webkit-animation-delay:.2s;animation-delay:.2s}
#object_four{top:29px;left:31px;-webkit-animation-delay:.3s;animation-delay:.3s}
#object_five{top:35px;left:17.5px;-webkit-animation-delay:.4s;animation-delay:.4s}
#object_six{top:29px;left:4px;-webkit-animation-delay:.5s;animation-delay:.5s}
#object_seven{top:17.5px;left:0px;-webkit-animation-delay:.6s;animation-delay:.6s}
#object_eight{top:6px;left:4px;-webkit-animation-delay:.7s;animation-delay:.7s}
@-webkit-keyframes animate{25%{-ms-transform:scale(1.5);-webkit-transform:scale(1.5);transform:scale(1.5)}
75%{-ms-transform:scale(0);-webkit-transform:scale(0);transform:scale(0)}
}
@keyframes animate{50%{-ms-transform:scale(1.5,1.5);-webkit-transform:scale(1.5,1.5);transform:scale(1.5,1.5)}
100%{-ms-transform:scale(1,1);-webkit-transform:scale(1,1);transform:scale(1,1)}
}
