/*top*/
.top{height: 50px;background: #C7000A;width: 100%;position: relative;position: fixed;left: 0px;top: 0px;z-index: 9999;}
.top div{text-align: center;}
.top_search{width: 68.4375%;background: none;margin: 0px auto;padding-top: 8.5px;position: relative;}
.top_searchinput{width: 94%;height: 33px;border: none;background: #FAE6E7;border-radius: 5px;font-size: 90%;padding: 0px 3%;}
.top_scan{position: absolute;top: 5.5px;left: 3%;text-align: center;}
.top_color{font-size: 80%;color: #EEB3B6;}
.top_news{position: absolute;top: 5.5px;right: 3%;text-align: center;width: 36px;}
.top_news_num{background: #FF6500;color: #FFFFFF;position: absolute;top: -2px;left: 50%;font-size: 70%;line-height: 100%;padding: 5% 12%;}
.top_searchico{position: absolute;top: 13px;right: 10px;}
.top_scan img{width: 20px;height: 19px;}
.top_news img{width: 20px;height: 20px;}
.top_searchico img{width: 17.5px;height: 17.5px;}
/* navigation */
.navigation{background: #FFFFFF;padding-bottom: 4%;}
.navigation ul li{float: left; width: 25%;text-align: center;margin-top: 4%;}
.navigation_ico{width: 55%;display: inline-block;}
.navigation_title{color: #666666;margin-top: 2%;}
.navigation ul li div{text-align: center;}
/* hotnews */
.hotnews{background: #FFFFFF;border-top: 1px solid #EDEDED;width: 100%;position: relative;}
.hotnews > div{float: left;
display: -webkit-box;-webkit-box-orient: vertical;-webkit-box-pack: center;
display: -moz-box;-moz-box-orient: vertical;-moz-box-pack: center;
display: -o-box;-o-box-orient: vertical;-o-box-pack: center;
display: -ms-box;-ms-box-orient: vertical;-ms-box-pack: center;
display: box;box-orient: vertical;box-pack: center;
}
.hotnews_l{width: 15.625%;position: absolute;top: 0px;bottom: 0px;left: 0px;}
.hotnews_r{width: 80.375%;margin-left:15.625% ;position: relative;padding:1% 0px 1% 4%;font-size: 90%;}
.hotnews_l span{display: block;text-align: center;line-height: 100%;font-style: italic;color: #C7000B;font-weight: 900;}
.hotnews_r::before{content: '';width: 1px;height: 100%;background: #EDEDED;position: absolute;top: 0px;left: 0px;}
.hotnews_r ul li{line-height: 100%;margin: 2% 0px;}
.hotnews_r ul li span{margin-right: 1%;}
.hotnews_r ul li span img{width: 13px;height: 13px;vertical-align: bottom;}
/* Actidraw */
.Actidraw{background: #FFFFFF;margin-top: 3%;padding: 3% 1.875%;}
.Actidrawtop{}
