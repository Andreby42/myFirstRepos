@charset "utf-8";
/* CSS Document */

/* top  开始*/
#top{ height:37px; line-height:37px; width:100%; background:#F7F9F8;}
#top .wrapper{ width:1200px; height:37px; margin:0 auto; position:relative;}
#top .wrapper .top-left{}
#top .wrapper .top-left .message{ width:16px; height:9px; background:url(../images/icion.png) no-repeat -120px -30px; display:inline-block;}
#top .wrapper .top-left .huise{ margin:0px 5px; color:#CCC; font-size:12px;}
#top .wrapper .top-right{ width:480px; height:37px; position:absolute; top:0px; right:0px; text-align:right;}
#top .wrapper .top-right ul li{ float:right; margin-left:15px; line-height:37px; }
#top .wrapper .top-right .shopping-cart{ width:20px; height:12px; background:url(../images/icion.png) no-repeat -200px -25px; display:inline-block;}
/* top  结束*/

/*banner  开始*/
#banner{ width:100%; height:68px; line-height:68px; background:#0199EF; font-size:16px;}
#banner .wrapper{ width:1200px; margin:0 auto; position:relative;}
#banner .wrapper .logo{ width:80px; height:55px; background:url(../images/icion.png) no-repeat 0px 0px; float:left; margin:10px 0px 0px 15px;}
#banner .wrapper .navigation{ width:350px; float:left; margin-left:125px;}
#banner .wrapper .navigation ul li{ float:left;line-height:68px; height:68px;}
#banner .wrapper .navigation ul li a{ color:#F2F9FF;}
#banner .wrapper .navigation ul .account-set{ margin:0px 75px; position:relative;}
#banner .wrapper .navigation ul .account-set .zhsz-con{ background:#FBFBFB; width:90px;position:absolute; top:22px; left:0px; border:1px solid #D6D6D6; padding: 15px 0px 5px 15px; line-height: normal; display:none; z-index:80; font-size:14px;}
#banner .wrapper .navigation ul .account-set .zhsz-con .zhsz-con-ul li{ line-height:normal; margin-bottom:5px; height:auto;}
#banner .wrapper .navigation ul .account-set .zhsz-con .zhsz-con-ul .zhsz-con-ul-title{ font-weight:bolder;}
#banner .wrapper .navigation ul .account-set .zhsz-con .zhsz-con-ul .zhsz-con-ul-title img{ margin-left:10px; vertical-align:text-top; margin-top:5px; }
#banner .wrapper .navigation ul .account-set .zhsz-con .zhsz-con-ul li a{ color:#565656;}
#banner .wrapper .navigation ul .account-set .zhsz-con .zhsz-con-ul li a:hover{ color:#c31;}
#banner .wrapper .navigation ul .home{ position:relative;}
#banner .wrapper .navigation ul .home .home-bac{ width:28px; height:17px; position:absolute;left:0px; bottom:0px; background:url(../images/arrow_index.png) no-repeat; }
#banner .wrapper .navigation ul li span.news{ min-width:10px; min-height:19px; background:#FFAE54; display:inline-block; color:#FFF; font-size:9px;text-align:center; position:absolute; top:18px; left:33px; line-height:19px; padding:0 5px;
-webkit-border-radius: 19px;
-moz-border-radius: 19px;
border-radius: 19px;}
.news-wb{ width:11px; height:4px; position:absolute; top:36px; left:40px; background: url(../images/news_01.png) no-repeat;}
#banner .wrapper .navigation ul .xiaoxi{ position:relative;}
#banner .wrapper .banner-right{ width:403px; height:68px; position:absolute; top:20px; right:0px; line-height:normal;}
#banner .wrapper .banner-right .searh{ width:217px; height:31px; background:url(../images/search.png) no-repeat; float:left;}
.searh-inp{ background:none; border:none; height:29px; width:162px; float:left; padding-left:3px;}
.searh-but{ background:none; border:none; height:30px; width:48px;  color:#565656; cursor:pointer; }
#banner .wrapper .banner-right .wd-shop{ width:165px; height:31px; background:url(../images/gwc.png) no-repeat; float:left; cursor:pointer; position:relative; margin-left:20px; }
#banner .wrapper .banner-right .wd-shop .news{ min-width:10px; min-height:19px; background:#FFAE54; display:inline-block; color:#FFF; font-size:9px;text-align:center; position:absolute; top:-13px; left:113px; line-height:19px; padding:0 5px;
-webkit-border-radius: 19px;
-moz-border-radius: 19px;
border-radius: 19px;}
#banner .wrapper .banner-right .wd-shop .news-wb{ width:11px; height:4px; position:absolute; top:5px; left:118px; background: url(../images/news_01.png) no-repeat; display:inline-block;}
/*banner  结束*/


