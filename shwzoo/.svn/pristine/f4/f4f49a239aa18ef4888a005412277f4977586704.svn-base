    <body>
<!--main  主要内容  开始 -->
<div id="main" class="fix">
    <!--main-left 左边内容  开始 -->
   <% include("/template/pc/memberCenter/Shared/_PartialLeftside.html",{}){}%>
    <!--main-left 左边内容  结束 -->
    <!--rightside 右边内容  结束 -->
    <div class="rightside">
        <!--l_jfmx积分明细  begin -->
        <div class="l_jfmx mt0">
            <!--l_jfmxtop  begin -->
            <div class="l_jfmxtop">
                <h3>
                    <a class="active" >会员反馈</a>
                </h3>
            </div><!--l_jfmxtop  stop -->
            <!--jfmxcon  begin -->
            <div class="jfmxcon">
            
                <table class="l_tablemost">
                    <tr id="tr_listtitle">
                        <th width="10%">标题</th>
                        <th width="50%">内容</th>
                        <th>状态 </th>
                        <th>创建时间 </th>
                        <th>操作</th>
                        
                    </tr>
                    <script type="text/html" id="completelist">
                        {{each list as pro i}}
                        <tr>
                            <td class="shenlan">{{pro.title}}</td>
                            <td class="shenlan"width="60%">{{pro.content}}</td>
                            
                            <td class="shenlan">
								{{if pro.status==0}} 
									未读
								{{else if pro.status==1}}
									已读
 								{{/if}} 
							</td>
							<td class="shenlan">{{pro.creattime | toDateTime}}</td>
							<td  class="shenlan" style="cursor: pointer;"><a href="/member/khFwmemb/feedBackdetail.html?id={{pro.id}}" >查看</a></td>
                        </tr>
                        {{/each}}
                    </script>
                        
                
                </table>
                <!--sppj_page  begin -->
                <div class="pager" id="pager">
                </div>
                <!--sppj_page  stop -->
            </div>
            <!--jfmxcon stop -->
        </div>
      
    </div>
    <!--rightside 右边内容  结束 -->
    <div class="clear"></div>
</div>
</body>
<!--main  主要内容  结束 -->
