package com.yinlian.Enums;

/** 密码强度 */
public enum StrengthPwdEnum {
	/** 无效密码 */
	无效密码(0),
	/** 低强度密码 */
	低强度密码(1),
	/** 中强度密码 */
	中强度密码(2),
	/** 高强度密码 */
	高强度密码(3);
	private final int value;

	public int getValue() {
		return value;
	}

	// 构造器默认也只能是private, 从而保证构造函数只能在内部使用
	StrengthPwdEnum(int value) {
		this.value = value;
	}
}
