@charset "utf-8";
/* CSS Document */

.data_list_detailed tr td{ text-align:left; padding-left:15px;}
.mainright .ddgl-table-con .data_list_detailed{ text-align:center;}
.preserve-inp{ width:80px; height:40px; border:1px solid #ccc; border-radius:5px; background:#efefef; font-size:18px; font-weight:bolder;}
.touxi-dpgl-wgdp{ width:42px; height:36px; background: url(../images/index_icion.png) no-repeat -90px -79px; display:inline-block; margin-top:25px;}
.weather-left-ddgl{ width:325px; float:left; color:#000; }
.weather-left-ddgl table tr td{ height:50px; background:#E4E4E4; color:#666;font-size:18px; font-weight:bolder; text-align:center;}
/* 结算单 begin*/
.account-form{ margin-top:35px; width:100%; height:30px; position:relative; color:#333;}
.account-form-search{ width:300px; height:30px; position:absolute; left:0px; top:0px; }
.account-form-inp{ width:200px; height:25px; background:#FFFFFF; border:1px solid #D4D4D4; padding-left:5px; color:#CCC;}
.account-form-but{ width:100px;background:#F5F5F5; border:1px solid #D4D4D4; height:29px; }
.sea-dw{ width:100px; height:25px; position:absolute; left:200px; top:0px;}
.account-form-select{ width:800px; height:30px; position:absolute; top:0px; left:330px;}
.inp-seller{ width:150px; height:21px;}
.account-con{ width:98%; margin-top:30px; color:#000; font-size:16px; font-family:"宋体"; color:#333;}
.account-con-mk{ width:100%; margin-bottom:30px;}
.account-con-mk-title{ width:100%; height:35px; line-height:35px; background:#E4E4E4; border-top:1px solid #BEBEBE; border-right:1px solid #BEBEBE; border-left:1px solid #BEBEBE; position:relative;  color:#333;}
.order-number{ width:310px; height:35px; position:absolute; top:0px; left:20px;}
.store-title{ width:310px; height:35px; position:absolute; top:0px; left:45%;}
.order-date{ width:250px; height:35px; position:absolute; top:0px; right:20px;}
.account-con-mk-tab{ width:100%; border-top:1px solid #D2D2D2; border-right:1px solid #BEBEBE; border-left:1px solid #BEBEBE;}
.account-con-mk-tab table{ border-collapse:collapse; font-size:14px;}
.account-con-mk-tab table, .account-con-mk-tab table tr, .account-con-mk-tab table tr td{ border:1px solid #E4E4E4;}
.account-con-mk-tab table tr td{ padding:15px 20px; text-align:center;}
.mainright .account-con .account-con-mk .account-con-mk-tab table tr .text-left{ font-size:12px;}
.mainright .account-con .account-con-mk .account-con-mk-tab table tr .text-left a:hover{ color:#c31;}
.account-con-mk-bottom{ width:100%; height:35px; line-height:35px; background:#E4E4E4; border-bottom:1px solid #BEBEBE; border-right:1px solid #BEBEBE; border-left:1px solid #BEBEBE; position:relative;color:#333;}
.money{ width:130px; height:35px; position:absolute; top:0px; right:500px; }
.money1{ width:130px; height:35px; position:absolute; top:0px; right:350px; }
.money2{ width:130px; height:35px; position:absolute; top:0px; right:180px; }
.money3{ width:130px; height:35px; position:absolute; top:0px; right:20px; }
/* 结算单 stop */

