package com.yinlian.wssc.web.dto;

/**
 * 全文检索数据集合
 * 
 * @author Administrator
 *
 */
public class ProLuceneDto {

	private Integer brandid; // 品牌ID
	private String brandname; // 品牌名称
	private Integer classid; // 分类ID
	private String classname; // 分类名称
	private Integer id; // spuid
	private Integer shopid; // 店铺ID
	private String shopname; // 店铺名称
	private String strkeys; // 规格值集合
	private String name; // 商品名称
	private String shorttitle; // 商品短标题
	private String spudesc; // 商品描述
	private String tag; // 商品标签
	private String fullpath; // 商品全路径

	public Integer getBrandid() {
		if (brandid == null)
			return -1;
		return brandid;
	}

	public void setBrandid(Integer brandid) {
		this.brandid = brandid;
	}

	public String getBrandname() {
		if (brandname == null)
			return "";
		return brandname;
	}

	public void setBrandname(String brandname) {
		this.brandname = brandname;
	}

	public Integer getClassid() {
		if (classid == null)
			return -1;
		return classid;
	}

	public void setClassid(Integer classid) {
		this.classid = classid;
	}

	public String getClassname() {
		return classname;
	}

	public void setClassname(String classname) {
		this.classname = classname;
	}

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public Integer getShopid() {
		if (shopid == null)
			return -1;
		return shopid;
	}

	public void setShopid(Integer shopid) {
		this.shopid = shopid;
	}

	public String getShopname() {
		if (shopname == null)
			return "";
		return shopname;
	}

	public void setShopname(String shopname) {
		this.shopname = shopname;
	}

	public String getStrkeys() {
		if (strkeys == null)
			return "";
		return strkeys;
	}

	public void setStrkeys(String strkeys) {
		this.strkeys = strkeys;
	}

	public String getName() {
		if (name == null)
			return "";
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getShorttitle() {
		if (shorttitle == null)
			return "";
		return shorttitle;
	}

	public void setShorttitle(String shorttitle) {
		this.shorttitle = shorttitle;
	}

	public String getSpudesc() {
		if (spudesc == null)
			return "";
		return spudesc;
	}

	public void setSpudesc(String spudesc) {
		this.spudesc = spudesc;
	}

	public String getTag() {
		return tag;
	}

	public void setTag(String tag) {
		this.tag = tag;
	}

	public String getFullpath() {
		return fullpath;
	}

	public void setFullpath(String fullpath) {
		this.fullpath = fullpath;
	}

}
