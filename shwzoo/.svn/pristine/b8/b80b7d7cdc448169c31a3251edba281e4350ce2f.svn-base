	<select id="selectOrderBillsByPage" resultMap="BaseResultMap"
		parameterType="com.yinlian.wssc.web.util.PageBeanUtil">
		select
		<include refid="Base_Column_List" />
		from orderbills
		<where>
			<if test="criteria.shopid != null and criteria.shopid >0">
				and shopid = #{criteria.shopid}
			</if>
			<if test="criteria.status != null">
				and Status=#{criteria.status}
			</if>
			<if test="criteria.billstart !=null and criteria.billstart !=''">			
			  <![CDATA[ and DATE_FORMAT(createdate,'%Y-%m-%d') >= #{criteria.billstart} ]]>
		   </if>
		   <if test="criteria.billsend !=null and criteria.billsend !=''">
		     <![CDATA[ and DATE_FORMAT(createdate,'%Y-%m-%d') <= #{criteria.billsend} ]]>
