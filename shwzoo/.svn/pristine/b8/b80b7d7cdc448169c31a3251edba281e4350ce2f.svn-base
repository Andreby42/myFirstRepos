  <select id="selectBills" resultType="com.yinlian.wssc.web.po.OrderBills" parameterType="com.yinlian.wssc.search.BillsOrderCriteria" >
    select o.Shopid as shopid,s.Name as shopname ,
    date_format(`o`.`AddOrderDate`,'%Y-%m-%d') AS orderdate,
    sum(od.ProductPrice) as orderprice
    from orderdetail od 
    left join orders o on od.OrderID = o.ID
    left join shop s on o.ShopId=s.id
    <if test="criteria != null ">
		<include refid="criteria_query" />
	</if>
    group by o.ShopId,DATE_FORMAT(o.AddOrderDate, "%Y-%m-%d" )
 
  </select>
  	<!-- 以下是自定义的 接口 -->
	<sql id="criteria_query">
		<where>
			<if test="criteria.orderstatu != null and criteria.orderstatu != '-1' ">
				and od.status = #{criteria.orderstatu}
			</if>
			<if test="criteria.deliverConfirmDate != null and criteria.deliverConfirmDate != '' ">
				 <![CDATA[ and DATEDIFF(o.DeliverConfirmDate,#{criteria.deliverConfirmDate})<=0]]>
			</if>
			and (od.isaddbill =0 or od.isaddbill is null) 
			and (od.VaildFlag =0 or od.VaildFlag is null)
			and (o.ValidFlag =0 or o.ValidFlag is null)
		</where>
	</sql>
	
