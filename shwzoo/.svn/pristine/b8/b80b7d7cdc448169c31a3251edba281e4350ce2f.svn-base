	<select id="selectBillsOrderByPage" resultType="com.yinlian.wssc.web.dto.BillsOrderDto"
		parameterType="com.yinlian.wssc.web.util.PageBeanUtil">
		select o.GroupCode as groupcode, od.ProductPrice as price,
		o.PayType as payType,od.Status as status,
		o.AddOrderDate as createtime,u.UserName as membername
		from orderdetail od 
		left join orders o on od.OrderID = o.ID
		left join users u on o.BuyerID=u.id
		<where>
			<if test="criteria.shopid != null and criteria.shopid >0">
				and o.shopid = #{criteria.shopid}
			</if>
			<if test="criteria.orderdate != null and criteria.orderdate!=''">
				and o.AddOrderDate like '%${criteria.orderdate}%'
			</if>
			<if test="criteria.status != null">
				and od.Status=#{criteria.status}
			</if>
		  and (o.ValidFlag =0 or o.ValidFlag is null)
		  and (od.VaildFlag =0 or od.VaildFlag is null)
		</where>
