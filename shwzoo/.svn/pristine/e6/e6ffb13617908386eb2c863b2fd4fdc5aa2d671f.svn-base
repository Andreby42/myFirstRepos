@charset "utf-8";
/* CSS Document */

/*所有分类 hc_lnav导航  begin*/
.hc_lnav{z-index:9999;position:relative;width:1200px;margin:0 auto;}
.hc_lnav .allbtn{z-index:99999;position:relative}
.hc_lnav .allbtn h2{font-size:14px;box-shadow:2px 0px 6px -3px #428000;-webkit-box-shadow:2px 0px 6px -3px #428000;-moz-box-shadow:2px 0px 6px -3px #428000}
.hc_lnav .allbtn h2 a{line-height:36px;background-color:#358000;padding-left:10px;width:180px;display:block;height:36px;color:#ffffff;font-size:14px;font-weight:normal}
.hc_lnav .allbtn h2 a:hover{background-color:#358000;text-decoration:none}
.hc_lnav .allbtn h2 i{}
.hc_lnav .allbtn h2 strong{}
.hc_lnav .allbtn ul{z-index:99999;position:absolute;background-color:#fff;width:190px;min-height:621px;top:0px;left:0px; border:1px solid #CDCAC5;}
body.hc_home .hc_lnav .allbtn ul{display:block}
body.hc_list .hc_lnav .allbtn ul{display:block}
.hc_lnav .allbtn ul li{padding:0 15px;zoom:1;clear:both;cursor:default; width:187px; margin:0 auto;}
.hc_lnav .allbtn ul li .tx{font-weight:bolder; padding-top:30px; font-size:14px; color:#000; height:35px;}
.hc_lnav .allbtn ul li dl{zoom:1;clear:both;overflow:auto; border-bottom:1px dashed #efefef; padding-bottom:30px;}
.hc_lnav .allbtn ul li dl a{line-height:22px;white-space:nowrap;float:left;color:#6F6D6D; margin-right:10px;-webkit-transition:color 0.1s ease-out 0s;-moz-transition:color 0.1s ease-out 0s;-ms-transition:color 0.1s ease-out 0s;-o-transition:color 0.1s ease-out 0s;transition:color 0.1s ease-out 0s}
.hc_lnav .allbtn ul li dd{line-height:22px;float:left;}
.hc_lnav .allbtn ul li .pop{position:absolute;background-color:#fff;min-height:621px;min-width:214px;display:none;height:621px; border:1px solid #D82423; border-left:none;}
.hc_lnav .allbtn:hover ul{display:block}
.hc_lnav .allbtn ul li:hover{background-color:#D82423; color:#fff;}
.hc_lnav .allbtn ul li:hover .tx{background-color:#D82423; color:#fff;}
.hc_lnav .allbtn ul li:hover .pop{display:block;top:-1px;left:215px}
.hc_lnav .allbtn ul li:hover .pop dl a.un{color:#a5a5a5;text-decoration:none}
.hc_lnav .allbtn ul li:hover dl{color:#6e6e6e; border-bottom:none;}
.hc_lnav .allbtn ul li:hover a{color:#fff}
.hc_lnav .allbtn ul li:hover a:hover{color:#fff; text-decoration:underline;}
.hc_lnav .allbtn ul:hover{ border:1px solid #D82423; border-right:1px solid #CDCAC5;}
.popmk{ height:601px; padding:0px 0px 20px 20px; float:left; margin-right:40px;}
#navigation .hc_lnav.jslist .allbtn .jspop.box .a1 .pop h3{ color:#D82423; font-size:20px; padding:20px 0px 0px 20px;}
.popmk dl dt{ font-size:14px; color:#000; font-weight:bolder; height:38px;}
.jspop.box .a1 .pop .popmk dl{ border-bottom:none; padding-bottom:10px;}
.jspop.box .a1 .pop .popmk dl dd{ display:block; float:none;}
.jspop.box .a1 .pop .popmk dl dd a{ color:#6F6D6D; display:block;float:none;}
.jspop.box .a1 .pop .popmk dl dd a:hover{color:#E81122; text-decoration:underline;}
