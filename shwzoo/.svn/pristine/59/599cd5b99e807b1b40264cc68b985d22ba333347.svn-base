<title>中绿味道-菜谱大全</title>
<script type="text/javascript">
//获取x详情
function getinfo(){
	var cid=GetQueryStringByName("cid");
	$.ajax({
        type: "post",
        url: "/zlnews/newsinfo/getnewsbyid",
        dataType: "json",
        data: {id:cid},
        success: function (rsl) {
            if (rsl.code == 0) {
                var infonews =  rsl.data.content;                   
                $(".cpdq-arcnr-l").append(infonews);
               /*  $(".l_jtyb").after(rsl.data.title);
                $(".cfbk-tit").html(rsl.data.title);
                $("#newstime").html(rsl.data.updatetime); */
             }
           }                  
      })
                
}
</script>
</head>
<body class="bodybjhs">
<!--navigation  导航开始 -->
