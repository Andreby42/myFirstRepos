package com.yinlian.pc.dto;

import java.util.List;

import com.yinlian.wssc.web.po.ProductImgs;

/**
 * pc端商品列表
 * @author Administrator
 *
 */
public class PCProListDto {
	
	/***
	 * 品牌id
	 */
	private int brandid;
	/***
	 * 标准商品id
	 */
	private int spuid;
	
	/***
	 * 商品名称
	 */
	private String name;
	
	/***
	 * 是否直营
	 */
	private Boolean isowned;
	
	/***
	 * 商品价格
	 */
	private double price;
	
	/***
	 * 商品分类id
	 */
	private int classid;
	
	/***
	 * 评论数
	 */
	private int commentcount;
	
	/***
	 * 店铺id
	 */
	private int shopid;
	
	/***
	 * 店铺名称
	 */
	private String shopname;
	
	/***
	 * 商品销量
	 */
	private int salescount;
	
	/***
	 * 商品图片列表
	 */
	private List<ProductImgs> images;
	
	private String imgUrl;

	
	public int getBrandid() {
		return brandid;
	}

	public void setBrandid(int brandid) {
		this.brandid = brandid;
	}

	public int getSpuid() {
		return spuid;
	}

	public void setSpuid(int spuid) {
		this.spuid = spuid;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Boolean getIsowned() {
		return isowned;
	}

	public void setIsowned(boolean isowned) {
		this.isowned = isowned;
	}

	public double getPrice() {
		return price;
	}

	public void setPrice(double price) {
		this.price = price;
	}

	public int getClassid() {
		return classid;
	}

	public void setClassid(int classid) {
		this.classid = classid;
	}

	public int getCommentcount() {
		return commentcount;
	}

	public void setCommentcount(int commentcount) {
		this.commentcount = commentcount;
	}

	public int getShopid() {
		return shopid;
	}

	public void setShopid(int shopid) {
		this.shopid = shopid;
	}

	public String getShopname() {
		return shopname;
	}

	public void setShopname(String shopname) {
		this.shopname = shopname;
	}

	public int getSalescount() {
		return salescount;
	}

	public void setSalescount(int salescount) {
		this.salescount = salescount;
	}

	public List<ProductImgs> getImages() {
		return images;
	}

	public void setImages(List<ProductImgs> images) {
		this.images = images;
	}

	public String getImgUrl() {
		return imgUrl;
	}

	public void setImgUrl(String imgUrl) {
		this.imgUrl = imgUrl;
	}

}
