package com.yinlian.Enums;
/**
 * APP启动出弹窗的广告类型
 * @author licd
 *
 */
public enum PopupAdTypeEnum {
	外部链接(0), 店铺(1), 商品(2),景点(4),图文编辑(5);
	
	private final int value;

	public int getValue() {
		return value;
	}

	// 构造器默认也只能是private, 从而保证构造函数只能在内部使用
	PopupAdTypeEnum(int value) {
		this.value = value;
	}
}
