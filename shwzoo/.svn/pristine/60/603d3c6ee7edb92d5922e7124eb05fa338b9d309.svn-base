    <div class="rightside">
      <!--   <input type="hidden" id="userID" value="@ViewData["UserID"]" /> -->
        <!--l_jfmx  begin -->
        <div class="l_jfmx mt0 fix">
            <!-- -->
            <div class="xzshdz-top" id="adlist">
                <span class="xgyx-xtb"><a href="javascript:void(0);" id="addAddress">新增收货地址</a></span>
                您已创建 <span class="lvse bolder" id="adCount">0</span> 个收货地址，最多可创建 <span class="lvse bolder">20</span> 个
			
            </div><!--xzshdz-top  stop -->
            <script type="text/html" id="addresslist">
                {{each list as ad i}}
                <div class="xzshdz-tab">

                    <h3>收货人：{{ad.name}}</h3>

                    <table>
                        <tr>
                            <td class="xzshdz-tab-left">收货人</td>
                            <td>{{ad.name}}</td>
                        </tr>
                        <tr>
                            <td class="xzshdz-tab-left">所在地区</td>
                            <td>
                                {{ad.provincename}}&nbsp;
                                {{ad.cityname}}&nbsp;
                                {{ad.areaname}}&nbsp;&nbsp;&nbsp;
                            </td>
                        </tr>
                        <tr>
                            <td class="xzshdz-tab-left">地址</td>
                            <td>
                                {{ad.address}}
                            </td>
                        </tr>
                        <tr>
                            <td class="xzshdz-tab-left">手机</td>
                            <td>{{ad.mobile}}</td>
                        </tr>
                        <tr>
                            <td class="xzshdz-tab-left">固定电话</td>
                            <td>{{ad.telAreacode}}&nbsp;{{ad.telNumber}}&nbsp;{{ad.telNumber}}</td>
                        </tr>
                      
                    </table>
                    <div class="xzshdz-tab-bottom">

                        <input type="hidden" id="adID" value="{{ad.id}}">
                        <a class="defaultAd" href="javascript:void(0);" onclick='setDefault({{ad.id}})'>{{if ad.isdefault==0}}设为默认{{else}}默认地址{{/if}}</a>
                        <a class="editAd" href="javascript:void(0);" onclick='edit({{ad.id}})'>编辑</a>

                    </div>
                    <div class="xzshdz-tab-close"><input type="hidden"  id="adID" value="{{ad.id}}">
                <img onclick='shanchu({{ad.id}})' src="/resource/pc/member/images/close.png"  width="16" height="16"  id="deleteAd" class="deleteAd"></a></div>
                </div>
                {{/each}}
            </script>
           
            <!--xzshdz-tab  stop -->

         <!--   <div class="xzshdz-top" style="margin-top:-10px;">
