package com.yinlian.Enums;

/**
 * 组合商品审核类型
 * 
 * @author mjx
 *
 */
public enum PackageCheckEnum {
	未提交(0), 提交审核中(1), 审核通过(2), 审核不通过(3);
	private final int value;

	public int getValue() {
		return value;
	}

	// 构造器默认也只能是private, 从而保证构造函数只能在内部使用
	PackageCheckEnum(int value) {
		this.value = value;
	}
}
