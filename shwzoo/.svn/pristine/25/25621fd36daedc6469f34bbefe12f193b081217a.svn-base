<script type="text/javascript">
var pindex;
$(function(){
	pindex=2;
	$("#ul_commentlist").on("click",".lwq01img img",function(){
		var id=$(this).attr("dataid");
		$.each($(this).parent(".lwq01img").find("img"),function(i,item){
			if ($(item).attr("dataid")!=id) {
				$(item).removeClass("imgda");
			}
		});
		if ($(this).attr("class")==null||$(this).attr("class").indexOf("imgda")==-1) {
			$(this).addClass("imgda");
			$(this).parent().next().css("display","");
			$(this).parent().next().find("img").attr("src",$(this).attr("src"));
		}else{
			$(this).removeClass("imgda");
			$(this).parent().next().css("display","none");
		}
	});
	$("#searchsp").click(function() {
		var shopid=$("#h_shopid").val();
	    var sname=$("#sname").val();var pmin=$("#pmin").val();var pmax=$("#pmax").val();
	    window.location.href="/web/shop/details.html?shopId="+shopid+"&spuname="+sname+"&pricemin="+pmin+"&pricemax="+pmax;
	});
	
	$(".l_hyp").click(function() {
		gettopic();
	});
	
});
function gettopic(index){
	if(index){//不指定页数则默认pindex
		pindex=index;
	}
	   $.ajax({
            type : "post",
            url : "/pc/products/gettopiclist",
            dataType : "json",
            data : {
                "index" : pindex,
                "size" : 3
            },
            success : function(rsl) {
                if (rsl.code == 0) {
                   var data=rsl.data;
                   var ht="";
                   if(data.length>0){
                   	pindex+=1;
                   	for(var i=0;i<data.length;i++){
	                   	ht+="<li>";
	                	ht+="<a href='/web/products/proinfo.html?spuid="+data[i].spuid+"'>";
	                	ht+="<img src='"+data[i].img+"' alt='' width='114' height='114'>";
	                	ht+="<span>￥"+data[i].price.toFixed(2)+" </span>";
	                	ht+="</a>";
	                    ht+="</li>";
                    }
                    $("#topiclist").html(ht);
                   }else{
                   	if(pindex!=1)
                   	gettopic(1);
                   }
                } 
            },
            error : function(e) {
                //alert(e.statusText);
            }
        });
}


</script>
