package com.yinlian.pc.dto;

import java.util.List;

public class PCTopicDto {

    private Integer id;              //专题ID
	private Integer display;         //显示位置
	private Integer type;            //类型
	private String imgurl;           //专题图片
	public List<TopicSpuDto> spus;	
	
	public List<TopicSpuDto> getSpus() {
		return spus;
	}
	public void setSpus(List<TopicSpuDto> spus) {
		this.spus = spus;
	}
	
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public Integer getDisplay() {
		return display;
	}
	public void setDisplay(Integer display) {
		this.display = display;
	}
	public Integer getType() {
		return type;
	}
	public void setType(Integer type) {
		this.type = type;
	}
	public String getImgurl() {
		return imgurl;
	}
	public void setImgurl(String imgurl) {
		this.imgurl = imgurl;
	}
}
