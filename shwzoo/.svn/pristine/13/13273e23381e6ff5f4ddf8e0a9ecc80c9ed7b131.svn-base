@charset "utf-8";
/* CSS Document */

/* header头部开始 */
#header{ width:100%; height:90px;}
#header .wrapper{ width:1200px; height:100%; margin:0 auto;}
#header .wrapper .logo{ width:396px; height:90px; float:left;}
#header .wrapper .searh{ width:298px; height:40px; background:url(../images/searh.png) no-repeat; float:left; margin:36px 0 0 173px;}
.searh-inp{ background:none; border:none; width:230px; height:27px;line-height:27px\0; padding-left:8px; margin:5px 0 0 0; float:left;}
.searh-but{ width:50px; height:30px; margin:5px 0 0 0;background:none; border:none;}
#header .wrapper .admin{ width:320px; height:45px; float:left; margin:30px 0px 0px 0px; line-height:45px; font-size:14px; text-align:right;}
.admin-tp{ width:48px; height:45px; background:url(../images/admin.png) no-repeat; display:inline-block; vertical-align:top;}
#header .wrapper .admin a{ color:#37A1DD; font-weight:bolder;}
#header .wrapper .admin a:hover{ color:#37A1DD;}
#header .wrapper .admin .exit a:hover{ text-decoration:underline;}
/* header头部结束 */

/*banner导航 开始*/
#banner{ width:100%; background:#019AEA; height:50px; background: -webkit-linear-gradient(left, #019AEA , #48C1F0); background: -o-linear-gradient(left, #019AEA , #48C1F0); background: -moz-linear-gradient(left, #019AEA , #48C1F0); background:linear-gradient(left, #019AEA , #48C1F0);}
#banner .wrapper{ width:1200px; margin:0 auto;}
#banner .wrapper ul li{ float:left; font-size:16px;}
#banner .wrapper ul li a{color:#FFFFFF; height:50px; line-height:50px; padding:0 18px; display:inline-block; margin:0 1px;}
#banner .wrapper ul li a:hover{ color:#1FA1EC; background:#EFF8FD; background: -webkit-linear-gradient(left, #EFF8FD , #EEF7FC); border-top:2px solid #14A4EB; margin-top:-1px; height:49px;}
#banner .wrapper ul li.ban-hover{ background:#EFF8FD; background: -webkit-linear-gradient(left, #EFF8FD , #EEF7FC); color:#1FA1EC;  border-top:2px solid #14A4EB; height:50px; margin-top:-1px; height:49px;}
#banner .wrapper ul li.ban-hover a{color:#1FA1EC;}
/*banner导航 结束*/
