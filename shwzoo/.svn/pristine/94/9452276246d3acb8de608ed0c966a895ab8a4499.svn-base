                <h3>浏览历史<span><a id="a_morehis" href="/member/pcbrowsehistory/queryhistory">查看更多</a></span></h3>
                <!--l_gzspsy  begin -->
                <div class="l_gzspsy">
                <%if(!isEmpty(browsehistory)){%>
                    <div class="r_roll01 fix" id="div_yeshistory">
                        <span class="r_rollleft01" id="div_last"></span>
                        <div class="r_rollcon01">
                            <ul id="ul_hislist">
                                <%for(history in browsehistory){%>
                                   
                                    <li>
                                        <a href="javascript:void(0);"><img src="${history.imgurl}" title="${history.spuname}"></a>
                                        <p>￥ ${history.price}</p>
                                    </li>
                                    
                                 <%}%>
                            </ul>
                        </div>
                        <span class="r_rollright01" id="div_next"></span>
                    </div>
                <%}else{%>
                 <div class="l_nowddd" id="div_nohistory"><i class="l_nodd03"></i>您还没有浏览任何商品，<a href="/web/sy/sypt">去逛逛吧~</a></div>
                <%}%> 
               
                </div>
                <!--l_gzspsy  stop -->
            </div>
            <!--浏览历史结束 -->
           
