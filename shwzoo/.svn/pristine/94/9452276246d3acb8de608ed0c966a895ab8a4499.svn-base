                <div class="l_wdddsycon">
                <%if(isEmpty(orders)){%>
                    <div class="l_nowddd l_ggyd" id="div_nomyorders"><i class="l_nodd01"></i>您还没有订单，<a href="/web/sy/sypt">去逛逛吧~</a></div>
                    <%}else{%>
                    <%for(order in orders){%>
                    <div id="div_myorders">
                        
                            <div class="l_wddconmk">                   
                            <%if(order.lsitdetais.~size>1){%>
                               <div class="l_wddconmk01"><a href="/web/products/proinfo.html?spuid=${order.lsitdetais[0].spuid}"><img src="${order.lsitdetais[0].productimg}" title="${order.lsitdetais[0].productname}"></a></div> 
                            <%}else{%> 
                            <%for(product in order.lsitdetais){%>
                             <div class="l_wddconmk01"><a href="/web/products/proinfo.html?spuid=${product.spuid}"><img src="${product.productimg}" title="${product.productname}"></a></div> 
                            <%}}%> 
                               <!-- {{if orders.ReceiveInfo!=null}}-->
                                <%if(!isEmpty(order.consignee)){%>
                                <div class="l_wddconmk02" >${order.consignee}</div>
                                <%}else{%>
                                <div class="l_wddconmk02">未知</div>
                                <%}%>

                                <div class="l_wddconmk03">
                                    <p class="red bolder">￥${order.actualPay}</p>
                                    <p>
                                    <%if(order.paytype==0){%>
                                        未支付
                                         <%}else if(order.paytype==1){%>
                                        在线支付
                                        <%}else if(order.paytype==2){%>
                                        余额支付
                                         <%}else if(order.paytype==3){%>
                                        优惠券支付
                                         <%}else if(order.paytype==4){%>
                                        混合支付
                                        <%}else if(order.paytype==4){%>
                                        货到付款
                                        <%}else{%>
                                        无支付信息
                                        <%}%>
                                    </p>
                                </div>
                                <div class="l_wddconmk04">
                                    <p>${order.addorderdate,dateFormat="yyyy-MM-dd HH:mm:ss"}</p>
                                </div>
                                <div class="l_wddconmk05">
                                    <p class="red bolder">
                                      <%if(order.status==0){%>
                                        待付款
                                        <%}else if(order.status==1){%>
                                        待发货
                                       <%}else if(order.status==2){%>
                                        待收货
                                     <%}else if(order.status==3){%>
                                        已确认收货
                                       <%}else if(order.status==4){%>
                                       未付款取消申请
                                       <%}else if(order.status==5){%>
                                      未付款已取消
                                        <%}else if(order.status==6){%>
                                        已付款取消申请
                                        <%}else if(order.status==7){%>
                                        已付款已取消
                                        <%}else if(order.status==8){%>
                                      已完成
                                        <%}else if(order.status==9){%>
                                        申请退款退货中
                                        <%}else if(order.status==10){%>
                                       已退款
                                     <%}%>
                                    </p>
                                    <p class="yscmh"><a href="/member/order/showOrderDetail?orderid=${order.Id}">订单跟踪</a></p>
                                </div>
                                <div class="l_wddconmk06 yscmh"><a href="/member/order/showOrderDetail?orderid=${order.Id}">查看</a></div>
                            </div>
                        
                    </div>
                    <%}%>
               <%}%>
                </div><!--l_wdddsycon  stop -->
