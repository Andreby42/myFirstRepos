                <!--l_gzspsy  begin -->
                <!--l_gzspsy  stop -->
                <div class="l_gzspsy">
                <%if(has(spu)){%>
                <%if(isEmpty(spu)){%>
                    <div class="l_nowddd" id="noconlist"><i class="l_nodd02"></i>您还没有关注任何商品，<a href="/web/sy/sypt">去逛逛吧~</a></div>
                     <%}else{%>
                    <div class="r_roll fix" id="hasconlist">
                        <span class="r_rollleft" id="conlast"></span>
                        <div class="r_rollcon">
